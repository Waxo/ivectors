{
  "name": "ivectors",
  "version": "1.0.0",
  "description": "i-vectors system validation",
  "main": "ivectors.js",
  "repository": "https://github.com/Waxo/ivectors.git",
  "author": "Maxime Robin <contact.waxo@gmail.com>",
  "license": "MIT",
  "xo": {
    "esnext": true,
    "envs": [
      "browser",
      "node",
      "mocha"
    ],
    "space": 2,
    "rules": {
      "max-len": [
        "error",
        80,
        2
      ]
    },
    "ignores": [
      "Gruntfile.js"
    ]
  },
  "nyc": {
    "exclude": [
      "app/utils/logger.js",
      "app/utils/exec-async.js",
      "config/environment.js",
      "test/**/*.js"
    ]
  },
  "dependencies": {
    "bluebird": "^3.4.7",
    "chalk": "^1.1.3",
    "dotenv": "^4.0.0",
    "fs-extra": "^2.0.0",
    "global": "^4.3.1",
    "mocha": "^3.2.0",
    "moment": "^2.17.1",
    "progress": "^1.1.8",
    "sound-parameters-extractor": "^1.3.1",
    "wav-file-info": "^0.0.7",
    "winston": "^2.3.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "grunt": "^1.0.1",
    "grunt-contrib-watch": "^1.0.0",
    "grunt-exec": "^2.0.0",
    "grunt-spawn-process": "^0.1.1",
    "grunt-xo": "^0.15.0",
    "load-grunt-tasks": "^3.5.2",
    "rewire": "^2.5.2",
    "time-grunt": "^1.4.0"
  },
  "scripts": {
    "test": "mocha --recursive",
    "clear-project": "rm -rf layers .nyc_output coverage",
    "start": "node ivectors.js"
  }
}
